#include<stack>
bool findRedundantBrackets(string &s){
stack<char> s;
char ch=s[i];
for(int i=0;i<s.length();i++){
if(ch=='('||ch=='+'||ch=='-'||ch=='*'||ch=='/'){
s.push(ch);
}
