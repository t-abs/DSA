#include <iostream>
using namespace std;
  
int main(){
    int a[50], reversed[50], n, i;
    cout << "Enter size of array";
    cin >> n;
    cout << "Enter elements of array";
    for(i = 0; i < n; i++){
        cin >> a[i];
    }
      
    for(i = 0; i <n; i++){
        reversed[i] =a[n-i-1];
    } 
    for(i = 0; i < n; i++){
        cout << reversed[i]<<" "  ;
    }
          return 0;
}
