//Search in BST
bool searchInBST(BinaryTreeNode<int> *root , int k) {
	//base case
	if(root==NULL){
		return false;
	}
	bool flag=false;
	if(k==root->data){
		return true;
	}
	if(k>root->data){
		flag=searchInBST(root->right,k);
	}
        if (k < root->data) {
          flag=searchInBST(root->left, k);

          return true;
        }
        return flag;
}
